      ],
      "datasourceTemplate": "maven",
      "depNameTemplate": "com.google.devtools.ksp:symbol-processing-gradle-plugin",
      "versioningTemplate": "semver"
    },
    {
      "fileMatch": ["README.md"],
      "matchStrings": [
        "\\|\\s*Paparazzi\\s*\\|\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+)\\s*\\|"
