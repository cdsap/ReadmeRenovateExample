{
   "packageRules": [
    {
        "matchDatasources": ["maven"],
        "registryUrls": [
            "https://dl.google.com/dl/android/maven2/",
            "https://plugins.gradle.org/m2/",
            "https://repo1.maven.org/maven2/"
        ],
        "automerge": true,
        "automergeType": "pr",
        "automergeStrategy": "merge-commit"
    }
  ],
  "regexManagers": [
    {
      "fileMatch": ["README.md"],
      "matchStrings": [
        "\\|\\s*Android Gradle Plugin\\s*\\|\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+)\\s*\\|"
      ],
      "datasourceTemplate": "https://maven.google.com/",
      "depNameTemplate": "com.android.tools.build:gradle",
      "versioningTemplate": "semver"
    },
    {
      "fileMatch": ["README.md"],
      "matchStrings": [
        "\\|\\s*Kotlin Gradle Plugin\\s*\\|\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+)\\s*\\|"
      ],
      "datasourceTemplate": "maven",
      "depNameTemplate": "org.jetbrains.kotlin:kotlin-gradle-plugin",
      "versioningTemplate": "semver"
    },
    {
      "fileMatch": ["README.md"],
      "matchStrings": [
        "\\|\\s*Anvil\\s*\\|\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+)\\s*\\|"
      ],
      "datasourceTemplate": "gradle",
      "depNameTemplate": "com.squareup.anvil:gradle-plugin",
      "versioningTemplate": "semver"
    },
    {
      "fileMatch": ["README.md"],
      "matchStrings": [
        "\\|\\s*Hilt\\s*\\|\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+)\\s*\\|"
